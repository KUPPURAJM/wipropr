
import java.util.Scanner;


public class sumofseries {
    
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        double sum=0;
        for(int i=1;i<=n;i++){
            if(i!=0){
                
                sum =  (int)sum+Math.pow(n,i)/i;
            }
          
        }
          System.out.println((int)sum);
    }
    
}
